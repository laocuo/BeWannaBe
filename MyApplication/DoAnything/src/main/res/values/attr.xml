<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="dotCount" format="integer" />
    <attr name="splitSize" format="integer" />
    <attr name="bg" format="reference"></attr>

    <declare-styleable name="CustomVolumControlBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="dotCount" />
        <attr name="splitSize" />
        <attr name="bg" />
    </declare-styleable>

    <declare-styleable name="namePokaViewGroup">
        <attr name="leftSize" format="integer"/>
        <attr name="topSize" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BinarySlidingMenu">
        <attr name="rightPadding" format="dimension"/>
    </declare-styleable>

    <attr name="icon" format="reference" />
    <attr name="color" format="color" />
    <attr name="text" format="string" />
    <attr name="text_size" format="dimension" />

    <declare-styleable name="ChangeColorIconView">
        <attr name="icon" />
        <attr name="color" />
        <attr name="text" />
        <attr name="text_size" />
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="item_visiable" format="integer" />
    </declare-styleable>

    <declare-styleable name="FolderLayout">
        <attr name="orientation" format="string" />
    </declare-styleable>
</resources>