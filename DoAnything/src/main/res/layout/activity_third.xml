<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customize="http://schemas.android.com/apk/res/com.example.doanything"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.doanything.view.impl.ThirdActivity">

    <com.example.doanything.view.CustomizedView.ColorfulPagerIndicator
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/pagerindicator_third"
        customize:item_visiable="4"/>
    <android.support.v4.view.ViewPager
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/viewpager_third">
    </android.support.v4.view.ViewPager>
</LinearLayout>
